{
  "parser": "babel-eslint",
  "plugins": ["react", "jsx-a11y", "prettier", "cypress"],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:jsx-a11y/recommended",
    "plugin:prettier/recommended",
    "prettier/react",
    "plugin:cypress/recommended"
  ],
  "settings": {
    "react": {
      "version": "16.11.0"
    }
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": false,
    "ecmaFeatures": {
      "globalReturn": false
    }
  }
}
